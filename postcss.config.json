{
  "plugins": [
    [
      "postcss-preset-env",
      {
        "autoprefixer": {
          "flexbox": "no-2009"
        },
        "stage": 3,
        "features": {
          "custom-properties": false,
          "nesting-rules": true,
          "custom-media-queries": true,
          "media-query-ranges": true,
          "cascade-layers": false
        }
      }
    ]
  ]
}
